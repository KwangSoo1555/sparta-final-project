{
  "family": "sparta-final-project-ecs-task",
  "taskRoleArn": "arn:aws:iam::590183983800:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::590183983800:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072",
  "containerDefinitions": [
    {
      "name": "sparta-final-project-backend-core-deployment",
      "image": "590183983800.dkr.ecr.ap-northeast-2.amazonaws.com/sparta-final-project-team7-server-origin",
      "cpu": 1024,
      "memory": null,
      "memoryReservation": 450,
      "portMappings": [
        {
          "containerPort": 3333,
          "hostPort": 3333,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "ulimits": [
        {
          "softLimit": 65535,
          "hardLimit": 65535,
          "name": "nofile"
        }
      ],
      "environment": [
        {
          "name": "APP_ENV",
          "value": "development"
        }
      ],
      "secrets": [
        {
          "name": "ENV_VAR",
          "valueFrom": "PROEJCT_ENV_VAR"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/sparta-final-project-ecs-task",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
